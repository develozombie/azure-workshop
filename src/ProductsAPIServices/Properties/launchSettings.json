    "ProductsAPIServices": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/Products",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:9002/"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/Products"
    }
  }
}